    "_back_build:socket-client": "yarn run simplism build --package socket-client",
    "_back_build:orm-service": "yarn run simplism build --package orm-service",
    "_back_build:orm-client": "yarn run simplism build --package orm-client",
    "_back_build:smtp-client-common": "yarn run simplism build --package smtp-client-common",
    "_back_build:smtp-client-service": "yarn run simplism build --package smtp-client-service",
    "_back_build:storage": "yarn run simplism build --package storage",
    "_back_build:cli": "yarn run simplism build --package cli",
    "_back_build:angular-hmr": "yarn run simplism build --package angular-hmr",
    "_back_build:angular": "yarn run simplism build --package angular"
