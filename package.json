{
  "name": "simplism",
  "version": "10.2.11",
  "description": "Simplism Packages Source Code",
  "repository": "github:kslhunter/simplism",
  "author": "김석래",
  "license": "MIT",
  "scripts": {
    "dev": "yarn run simplism build --watch",
    "test": "yarn run simplism test",
    "build": "yarn run simplism build",
    "publish": "yarn run simplism publish",
    "build+publish": "yarn run build && yarn run publish",
    "test+build+publish": "yarn run test && yarn run build && yarn run publish",
    "--  ": "",
    "build:core": "yarn run simplism build --package core",
    "build:excel": "yarn run simplism build --package excel",
    "build:orm-common": "yarn run simplism build --package orm-common",
    "build:orm-query": "yarn run simplism build --package orm-query",
    "build:orm-connector": "yarn run simplism build --package orm-connector",
    "build:socket-common": "yarn run simplism build --package socket-common",
    "build:socket-server": "yarn run simplism build --package socket-server",
    "build:socket-client": "yarn run simplism build --package socket-client",
    "build:orm-service": "yarn run simplism build --package orm-service",
    "build:orm-client": "yarn run simplism build --package orm-client",
    "build:smtp-client-common": "yarn run simplism build --package smtp-client-common",
    "build:smtp-client-service": "yarn run simplism build --package smtp-client-service",
    "build:storage": "yarn run simplism build --package storage",
    "build:cli": "yarn run simplism build --package cli",
    "build:angular-hmr": "yarn run simplism build --package angular-hmr",
    "build:angular": "yarn run simplism build --package angular",
    "--   ": "",
    "simplism": "cross-env TS_NODE_TRANSPILE_ONLY=true cross-env TS_NODE_PROJECT=packages/cli/tsconfig.json node --require ts-node/register --require tsconfig-paths/register --trace-warnings packages/cli/src/bin.ts",
    "reinstall": "git clean -xfd && (if exist \"yarn.lock\" del /s /q \"yarn.lock\") && yarn install",
    "--": "",
    "_back_test+build": "yarn run test && yarn run build",
    "_back_test+build+publish": "yarn run test && yarn run build && yarn run publish",
    "-- ": "",
    "_back_test:core": "yarn run simplism test --package core",
    "_back_test:excel": "yarn run simplism test --package excel",
    "_back_test:orm-query": "yarn run simplism test --package orm-query",
    "_back_test:orm-connector": "yarn run simplism test --package orm-connector",
    "_back_test:socket": "yarn run simplism test --package socket",
    "_back_test:orm-service": "yarn run simplism test --package orm-service",
    "_back_test:storage": "yarn run simplism test --package storage",
    "_back_test:angular": "yarn run simplism test --package angular"
  },
