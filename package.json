    "publish": "yarn run simplism publish",
    "build+publish": "yarn run build && yarn run publish",
    "--   ": "",
    "simplism": "cross-env TS_NODE_TRANSPILE_ONLY=true cross-env TS_NODE_PROJECT=packages/cli/tsconfig.json node --require ts-node/register --require tsconfig-paths/register --trace-warnings packages/cli/src/bin.ts",
    "reinstall": "git clean -xfd && (if exist \"yarn.lock\" del /s /q \"yarn.lock\") && yarn install",
    "--  ": "",
    "_back_build:core": "yarn run simplism build --package core",
    "_back_build:excel": "yarn run simplism build --package excel",
    "_back_build:orm-common": "yarn run simplism build --package orm-common",
